<Configuration>
    <Networking>Enable</Networking>
    <VGpu>Enable</VGpu> 
    <MappedFolders>
        <MappedFolder>
            <HostFolder>C:\Temp</HostFolder>
            <ReadOnly>true</ReadOnly>
        </MappedFolder>
    </MappedFolders>
    <LogonCommand>
        <Command>powershell.exe -noexit -command "cd ~/Desktop/Temp; Set-ExecutionPolicy RemoteSigned" </Command>
    </LogonCommand>
</Configuration>