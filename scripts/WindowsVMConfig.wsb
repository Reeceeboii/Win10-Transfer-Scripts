<Configuration>
    <vGPU>Enable</vGPU>
    <MappedFolders>
        <MappedFolder>
            <HostFolder>C:\Temp</HostFolder>
            <ReadOnly>false</ReadOnly>
        </MappedFolder>
    </MappedFolders>
    <LogonCommand>
        <Command>powershell.exe -noexit -command "cd ~/Desktop/Temp"</Command>
    </LogonCommand>
    <MemoryInMB>8192</MemoryInMB>
</Configuration>